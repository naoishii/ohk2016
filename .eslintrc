{
  "env": {
    "browser": true,
    "mocha": true,
    "es6": true
  },
  "globals": {},
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "parser": null,
  "rules": {
    "no-empty": 2,
    "no-loop-func": 2,
    "space-after-keywords": [
      1,
      "always"
    ],
    "semi-spacing": 1,
    "eqeqeq": 2,
    "new-cap": 2,
    "no-const-assign": 2,
    "dot-notation": 1,
    "camelcase": 1,
    "comma-dangle": 0,
    "no-spaced-func": 2,
    "constructor-super": 2,
    "consistent-return": 0,
    "comma-spacing": 1,
    "indent": [
      1,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "space-before-blocks": [
      1,
      "always"
    ],
    "no-multi-spaces": 1,
    "no-extra-boolean-cast": 1,
    "space-infix-ops": 1,
    "semi": 2,
    "no-this-before-super": 2,
    "no-alert": 0,
    "no-unused-expressions": 2,
    "no-trailing-spaces": 1,
    "no-new": 2,
    "no-underscore-dangle": 0,
    "no-use-before-define": 2,
    "quotes": [
      1,
      "single",
      "avoid-escape"
    ],
    "brace-style": [
      1,
      "1tbs"
    ],
    "no-unused-vars": 2,
    "no-else-return": 0,
    "callback-return": 2,
    "dot-location": [
      1,
      "property"
    ],
    "no-var": 2,
    "space-before-function-paren": [
      1,
      "never"
    ],
    "no-throw-literal": 2,
    "default-case": 2
  }
}
